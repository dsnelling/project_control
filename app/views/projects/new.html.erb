<h1>New project</h1>

<%= form_for(@project) do |f| %>
  <%= render "shared/error_messages", :target => @project %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :inactive %><br />
    <%= f.check_box :inactive %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>
