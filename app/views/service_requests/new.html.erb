<h1>New service_request</h1>

<% form_for(@service_request) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :project %><br />
    <%= f.text_field :project %>
  </p>
  <p>
    <%= f.label :request_ref %><br />
    <%= f.text_field :request_ref %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :disclipline %><br />
    <%= f.text_field :disclipline %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :auth_cost %><br />
    <%= f.text_field :auth_cost %>
  </p>
  <p>
    <%= f.label :request_date %><br />
    <%= f.datetime_select :request_date %>
  </p>
  <p>
    <%= f.label :auth_date %><br />
    <%= f.datetime_select :auth_date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', service_requests_path %>