<html>
  <head>
    <title>Signin</title>
  </head>
  <body>
    <%= start_form_tag :action =>"signin" %>
	<table>
	  <tr>
	    <td><label for="name">Name:</label></td>
	    <td><%= text_field_tag "name" %></td>
	  </tr>
	  <tr>
	    <td><label for="password">Password:</td>
		<td><%= password_field_tag "password" %></td>
	  </tr>
	</table>
	<%= end_form_tag%>
  </body>
</html>

